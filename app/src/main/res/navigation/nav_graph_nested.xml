<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_nested"
    app:startDestination="@id/reposFragment">

    <fragment
        android:id="@+id/detalInfoFragment"
        android:name="com.example.skeletmvp.ui.view.fragments.DetalInfoFragment"
        android:label="fragment_detal_info"
        tools:layout="@layout/fragment_detal_info" >
        <action
            android:id="@+id/action_detalInfoFragment_to_reposFragment"
            app:destination="@id/reposFragment" />
    </fragment>
    <fragment
        android:id="@+id/reposFragment"
        android:name="com.example.skeletmvp.ui.view.fragments.repos.ReposFragment"
        android:label="fragment_repos"
        tools:layout="@layout/fragment_repos" >
        <action
            android:id="@+id/action_reposFragment_to_detalInfoFragment"
            app:destination="@id/detalInfoFragment" />
    </fragment>
</navigation>