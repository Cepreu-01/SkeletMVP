<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_nested"
    app:startDestination="@id/infoFragment">

    <fragment
        android:id="@+id/infoFragment"
        android:name="com.example.skeletmvp.ui.view.fragments.info.InfoFragment"
        android:label="fragment_info"
        tools:layout="@layout/fragment_info" >
        <action
            android:id="@+id/action_infoFragment_to_detalInfoFragment"
            app:destination="@id/detalInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/detalInfoFragment"
        android:name="com.example.skeletmvp.ui.view.fragments.DetalInfoFragment"
        android:label="fragment_detal_info"
        tools:layout="@layout/fragment_detal_info" >
        <action
            android:id="@+id/action_detalInfoFragment_to_infoFragment"
            app:destination="@id/infoFragment" />
    </fragment>
</navigation>